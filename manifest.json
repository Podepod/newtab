{
    "manifest_version": 2,
    "name": "newtab",
    "version": "1.2.2",
    "description": "a minimalistic newtab page",
    "author": "Podepod",
    "homepage_url": "https://github.com/Podepod/newtab#readme",
    "offline_enabled": true,
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
